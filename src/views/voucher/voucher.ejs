            <script src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>

            <div class="containerss">
              <div class="row">
                <table id="example" class="table table-striped table-bordered" style="width:100%">
                  <thead>
                    <tr>
                      <th><input type="checkbox" onclick="checkAll(this)"></th>
                      <th>id </th>
                      <th>Mã Voucher</th>
                      <th>Thể loại</th>
                      <th>Đã dùng</th>
                      <th>Bắt đầu</th>
                      <th>kết thúc</th>

                    </tr>
                  </thead>
                  <style>
                    .underline-on-hover:hover {
                      cursor: pointer;
                    }
                  </style>
                  <tbody>
                    <% vouchers.map((voucher)=>{ %>
                      <tr>
                        <td><input type="checkbox" name=""></td>
                        <td>
                          <%= voucher.id %>
                        </td>
                        <td>
                          <span style="font-size: 17px; color: rgb(50, 131, 189);">
                            <a style=" text-decoration: underline;" class="underline-on-hover">
                              <%= voucher.code %>
                            </a>

                          </span>

                          <div>
                            <%= voucher.reward_type %>
                          </div>
                        </td>
                        <td>
                          <%= voucher.voucher_type %>
                        </td>
                        <td>
                          <%= voucher.use_history %>
                        </td>
                        <td>
                          <%= voucher.start_time %>
                        </td>
                        <td>
                          <%= voucher.end_time %>
                        </td>
                      </tr>
                      <%})%>




                  </tbody>

                </table>
